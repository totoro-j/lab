 /*页内导航*/
.anchor{width:945px;text-align:center;margin:0 auto;}
.subpage_anchor li{border:1px solid #e2e2e2;
    background:#F7FDFA;
	margin-right:25px;
	margin-bottom:10px;
	float:left;
	height:76px;
	width:200px;
	text-align:center;
	line-height:76px; /*这里我把原来的padding-top去掉了，然后把它的30px加到了方格的height上面，文字的垂直居中只要把line-height设成高度一样的就行了*/
	-moz-transition:0.3s;     /*这里加了一点变化的时间*/
	-webkit-transition:0.3s;
	-o-transition:0.3s;
	transition:0.3s;}

.subpage_anchor li a{height:76px;width:200px;display:block;}
.subpage_anchor li a:hover{color:#fff;background:#23a96f;box-shadow:0px -1px 5px #777;opacity:0.9;transition:0.6s;}

.subpage_title{width:945px;border-left:15px solid #23a96f;padding-left:18px;font-size:19px;margin-top:50px;margin-bottom:50px;text-align:left;}

.subpage_anchor subpage_detail{width:945px;margin:0 auto;}
.subpage_anchor{border-top:2px solid #e2e2e2;padding-top:30px;margin-top:-25px;}
/*页内导航结束*/


